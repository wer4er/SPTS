            StatCounter.StatCounterGui:Destroy()
        end)
        
        local starttime = os.time()
        
        local function updatetime()
            StatCounter.TimeFrameText.Text = 'Time: ' .. formattime(os.time() - starttime)
        end
        
        local updating_stats = {
            {StatCounter.FsFrameText, 'FistStrength', LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value}, 
            {StatCounter.BtFrameText, 'BodyToughness', LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value}, 
            {StatCounter.PpFrameText, 'PsychicPower', LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value}, 
            {StatCounter.MsFrameText, 'MovementSpeed', LocalPlayer:FindFirstChild('PrivateStats').MovementSpeed.Value}, 
            {StatCounter.JfFrameText, 'JumpForce', LocalPlayer:FindFirstChild('PrivateStats').JumpForce.Value}, 
            {StatCounter.TokensFrameText, 'Token', LocalPlayer:FindFirstChild('PrivateStats').Token.Value}
        }
        
        local old_value = 0;
        local all_tokens = 0;
        
        for _, v in updating_stats do
            v[1].Text = v[1].Name .. ': 0'
            
            if v[2] ~= 'Token' then
                LocalPlayer:FindFirstChild('PrivateStats')[v[2]]:GetPropertyChangedSignal('Value'):Connect(function()
                    v[1].Text = v[1].Name .. ': ' .. converttoletter(LocalPlayer:FindFirstChild('PrivateStats')[v[2]].Value - v[3])
                end)
            else
                LocalPlayer:FindFirstChild('PrivateStats')[v[2]]:GetPropertyChangedSignal('Value'):Connect(function()
                    if LocalPlayer:FindFirstChild('PrivateStats')[v[2]].Value - old_value > 0 then
                        all_tokens = all_tokens + 5
                    end
                    old_value = LocalPlayer:FindFirstChild('PrivateStats')[v[2]].Value
                    v[1].Text = v[1].Name .. ': ' .. converttoletter(all_tokens)
                end)
            end
        end
        
        task.spawn(function()
            while task.wait(0.1) do
                updatetime()
            end
        end)
    end,
})

