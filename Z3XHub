local Window = Rayfield:CreateWindow({
    Name = 'Z3XHub',
    LoadingTitle = 'Z3XHub',
    LoadingSubtitle = 'by wer4er',
})

Farm = Window:CreateTab('Farm', 7733674079)
Farm:CreateSection('Farm')
Visual = Window:CreateTab('Visual', 7733919881)
Visual:CreateSection('Visual')
Usual = Window:CreateTab('Usual', 7734068321)
Usual:CreateSection('Usual')
Pvp = Window:CreateTab('Pvp', 7734058599)
Pvp:CreateSection('Pvp')
Teleports = Window:CreateTab('Teleports', 7734020989)
Teleports:CreateSection('Teleports')
Fun = Window:CreateTab('Fun', 7734059095)
Fun:CreateSection('Fun')
Server = Window:CreateTab('Server', 8997386997)
Server:CreateSection('Server')
Settings = Window:CreateTab('Settings', 7734053495)
Settings:CreateSection('Settings')

local targets = {}
local target = LocalPlayer.Name

Pvp:CreateInput({
    Name = 'Targets',
    PlaceholderText = 'Nickname',
    RemoveTextAfterFocusLost = false,
    Callback = function(Value)
        for _, v in pairs(Players:GetPlayers()) do
            if string.find(string.lower(v.Name), string.lower(Value)) then
                target = v.Name
                break
            end
        end
        Rayfield:Notify({
            Title = 'Selected',
            Content = target,
            Duration = 5,
         })
    end,
 })

Pvp:CreateButton({
    Name = 'Add',
    Callback = function()
        if Players:FindFirstChild(target) then
            if not table.find(targets, target) then
                table.insert(targets, target)
            end
        end
        Rayfield:Notify({
            Title = 'Targets',
            Content = table.concat(targets, ', '),
            Duration = 5,
        })
    end,
})

Pvp:CreateButton({
    Name = 'Remove',
    Callback = function()
        if table.find(targets, target) then
            for _, v in pairs(targets) do
                if v == target then
                    table.remove(targets, i)
                end
            end
        end
        Rayfield:Notify({
            Title = 'Targets',
            Content = table.concat(targets, ', '),
            Duration = 5,
        })
    end,
})

Pvp:CreateButton({
    Name = 'Clear',
    Callback = function()
        targets = {}
        Rayfield:Notify({
            Title = 'Targets',
            Content = table.concat(targets, ', '),
            Duration = 5,
        })
    end,
})

