Toggls.RgbLasers:OnChanged(function()
    while Flags['RGBLasers'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(workspace:GetChildren()) do
                if v.Name == 'Part' then
                    v.Transparency = 0
                    v.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                end
            end
        end)
    end
end)

Toggls.RgbNames:OnChanged(function()
    if Flags['RGBNames'] then
        while Flags['RGBNames'] and task.wait(0) do 
            for _, v in pairs(Players:GetPlayers()) do
                pcall(function()
                    if v.Character:FindFirstChild('Head'):FindFirstChild('NameBbGui')  then
                        v.Character.Head.NameBbGui.NameTxt.TextColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                        v.Character.Head.NameBbGui.NameTxt.TextStrokeColor3 = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                    end
                end)
            end
        end
    else
        task.wait(.1)
        for _, v in pairs(Players:GetPlayers()) do
            pcall(function()
                if v.Character:FindFirstChild('Head'):FindFirstChild('NameBbGui')  then
                    v.Character.Head.NameBbGui.NameTxt.TextColor3 = updateColor(v.leaderstats.Status.Value)
                    v.Character.Head.NameBbGui.NameTxt.TextStrokeColor3 = updateColorStroke(v.leaderstats.Status.Value)
                end
            end)
        end
    end
end)

