Toggls.Spin:OnChanged(function()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild('HumanoidRootPart') then
        for _, v in pairs(LocalPlayer.Character.HumanoidRootPart:GetChildren()) do
            if v.Name == 'Spin' then
                v:Destroy()
            end
        end
    end
    if Flags['Spin'] and LocalPlayer.Character then
        local body = Instance.new('BodyAngularVelocity')
        body.Name = 'Spin'
        body.Parent = LocalPlayer.Character.HumanoidRootPart
        body.MaxTorque = Vector3.new(0, math.huge, 0)
        body.AngularVelocity = Vector3.new(0, Flags['SpinSpeed'], 0)
    end
end)

Toggls.Gravity:OnChanged(function()
    if Flags['Gravity'] then
        workspace.Gravity = Flags['GravityValue']
    else
        workspace.Gravity = StockGravity
    end 
end)


respawnhp = 0.5

local RespawnHP = Farm:CreateSlider({
    Name = 'Respawn',
    Range = {1, 99},
    Increment = 1,
    Suffix = '%',
    CurrentValue = 50,
    Flag = 'RespHP',
    Callback = function()
        respawnhp = Flags['RespHP'] / 100
    end,
})

Farm:CreateButton({
    Name = 'Stats Checker',
    Callback = function()
        for _, v in pairs(LocalPlayer.PlayerGui:GetChildren()) do
            if v.Name == 'SPTSPlayer' then
                v:Destroy()
            end
        end
        task.spawn(function()
            pcall(function()
                loadstring(game:HttpGet('https://rawscripts.net/raw/Super-Power-Training-Simulator-darkhub-4134'))()
            end)
        end)
    end,
})

