Toggls.Fist:OnChanged(function()
    while Flags['Fist'] and task.wait(0) do
        pcall(function()
            if getfspos() ~= nil then
                LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(getfspos())
            end
            for i = 1, #getfs() do
                RemoteEvent:FireServer({getfs()[i]})
            end
        end)
    end
end)

Toggls.Body:OnChanged(function()
    while Flags['Body'] and task.wait(0) do
        pcall(function()
            if getbtpos() ~= nil then
                LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(getbtpos())
            else
                RemoteEvent:FireServer({'+BT1'})
            end
        end)
    end
end)

Toggls.Psychic:OnChanged(function()
    while Flags['Psychic'] and task.wait(0) do
        pcall(function()
            if getpppos() ~= nil then
                LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(getpppos())
            end
            for i = 1, #getpp() do
                RemoteEvent:FireServer({getpp()[i]})
            end
        end)
    end
end)

Toggls.JumpSpeed:OnChanged(function()
    while Flags['JumpSpeed'] and task.wait(0) do
        pcall(function()
            RemoteEvent:FireServer({'Weight', 'Weight4'})
            RemoteEvent:FireServer({'+MS5'})
            RemoteEvent:FireServer({'+JF5'})
        end)
    end
end)

Toggls.Esp:OnChanged(function()
    if Flags['Esp'] then
        for _, v in pairs(Players:GetPlayers()) do
            task.spawn(function()
                if v ~= LocalPlayer then
                    if v.Character and v.Character.Humanoid and v.Character.Humanoid.Health > 0 then
                        CreateESP(v.Character, v)
                    end
                end
            end)
        end
    else
        game.CoreGui.ESPFolder:ClearAllChildren()
    end
end)

Toggls.Highlights:OnChanged(function()
    for _, v in pairs(Players:GetPlayers()) do
        if v ~= LocalPlayer then
            if Flags['Highlights'] then
                if v.Character and v.Character.Humanoid and v.Character.Humanoid.Health > 0 then
                    CreateHighlight(v.Character, v)
                end
            else
                if v.Character then
                    for _, v2 in pairs(v.Character:GetChildren()) do
                        if v2.Name == 'Highlight' then
                            v2:Destroy()
                        end
                    end
                end
            end
        end
    end
end)

Toggls.DayNight:OnChanged(function()
    Lighting.ClockTime = Flags['DayNight'] and 24 or 12
end)
    
Lighting:GetPropertyChangedSignal('ClockTime'):Connect(function()
    Lighting.ClockTime = Flags['DayNight'] and 24 or 12
end)

Toggls.Quests:OnChanged(function()
    while Flags['AutoClaimQuests'] and task.wait(0.25) do
        pcall(function()
            RemoteEvent:FireServer({'DLQ','MS','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'DLQ','PP','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'DLQ','FS','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'DLQ','BT','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'DLQ','JF','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','PP1','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','BT1','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','FS1','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','PP2','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','BT2','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','FS2','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','PP3','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','BT3','Claim'})
            task.wait(0)
            RemoteEvent:FireServer({'WLQ','FS3','Claim'})
        end)
    end
end)

Toggls.AnAura:OnChanged(function()
    while Flags['AntiLag'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(Players:GetPlayers()) do
                for _, v2 in pairs(v.Character:GetChildren()) do
                    if v2.Name == 'KillingIntentPart' then 
                        v2:Destroy()
                    end
                end
            end
        end)
    end
end)

Toggls.AnInv:OnChanged(function()
    while Flags['AntiInvisible'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(Players:GetPlayers()) do
                for _, v2 in pairs(workspace[v.Name]:GetChildren()) do
                    if v2:IsA('BasePart') and v2.Name ~= 'HumanoidRootPart' then
                        v2.Transparency = 0
                    end
                end
            end            
        end)
    end
end)

Toggls.ChatSPY:OnChanged(function()
    task.spawn(function()
        local spy = Flags['ChatSPY']
        local privateProperties = {Color = Color3.fromRGB(255, 255, 0), Font = 'GothamBold', TextSize = 20}
        local StarterGui = game.StarterGui
        local Players = Players
        local player = Players.LocalPlayer
        local instance = (chatSpyInstance or 0) + 1
        local chatSpyInstance = instance
        function onChatted(p, msg)
            if chatSpyInstance == instance then
                if spy then
                    msg = msg:gsub('[\n\r]', ''):gsub('\t', ' '):gsub('[ ]+', ' ')
                    local hidden = true
                    local conn = ReplicatedStorage:WaitForChild('DefaultChatSystemChatEvents', math.huge):WaitForChild('OnMessageDoneFiltering', math.huge).OnClientEvent:Connect(function(packet, channel)
                        if packet.SpeakerUserId == p.UserId and packet.Message == msg:sub(#msg - #packet.Message + 1) and (channel == 'All' or (channel == 'Team' and p.Team == player.Team)) then
                            hidden = false
                        end
                    end)
                    task.wait(1)
                    conn:Disconnect()
                    if hidden and spy then
                        privateProperties.Text = '{SPY} ['.. p.Name.. ']: '.. msg
                        StarterGui:SetCore('ChatMakeSystemMessage', privateProperties)
                    end
                end
            end
        end
        for _, v in pairs(Players:GetPlayers()) do
            v.Chatted:Connect(function(msg) onChatted(v, msg) end)
        end
        privateProperties.Text = '{SPY '.. (spy and 'EN' or 'DIS').. 'ABLED}'
        player:WaitForChild('PlayerGui', math.huge):WaitForChild('Chat', math.huge)
        StarterGui:SetCore('ChatMakeSystemMessage', privateProperties)
        task.wait(3)
        local chatFrame = player.PlayerGui.Chat.Frame
        chatFrame.ChatChannelParentFrame.Visible = true
        chatFrame.ChatBarParentFrame.Position = chatFrame.ChatChannelParentFrame.Position+UDim2.new(UDim.new(),chatFrame.ChatChannelParentFrame.Size.Y)
    end)
end)

Toggls.CBring:OnChanged(function()
    while Flags['CBring'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets) do
                if workspace:FindFirstChild(v) then
                    if not LocalPlayer.Character:FindFirstChild('ForceField') and not workspace[v]:FindFirstChild('ForceField') and not workspace[v]:FindFirstChild('SafeZoneShield') and workspace[v].Humanoid.Health ~= 0 and v ~= LocalPlayer then
                        workspace[v].HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 1
                    end
                end
            end
        end)
    end
end)

Toggls.AutoC:OnChanged(function()
    while Flags['AutoC'] and task.wait(0) do
        pcall(function()
            RemoteEvent:FireServer({'Skill_Punch', 'Left'})
            RemoteEvent:FireServer({'Skill_Punch', 'Right'})
        end)
    end
end)

Toggls.SpamR:OnChanged(function()
    while Flags['SpamR'] and task.wait(0) do
        pcall(function()
            RemoteEvent:FireServer({'Skill_SpherePunch', Vector3.new(0, 1e20, 0)})
            for _, v in pairs(workspace.Storage:GetChildren()) do
                if v.Name == getball() then
                    v.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
                end
            end
        end)
    end
end)

Toggls.Stack:OnChanged(function()
    task.spawn(function()
        if LocalPlayer.Character then
            RemoteEvent:FireServer({'Respawn'})
        end
        task.wait(1.4)
        if workspace:FindFirstChild('Town') and Flags['Stack'] then
            workspace:FindFirstChild('Town').Parent = LocalPlayer
        elseif LocalPlayer:FindFirstChild('Town') and not Flags['Stack'] then 
            LocalPlayer:FindFirstChild('Town').Parent = workspace
        end
    end)
    while Flags['Stack'] and task.wait(0) do
        pcall(function()
            RemoteEvent:FireServer({'Skill_SpherePunch', Vector3.new(0, 1e20, 0)})
            for _, v in pairs(targets) do   
                task.spawn(function()
                    local target = Players[v]
                    if target.Character and LocalPlayer.Character and (not LocalPlayer.Character:FindFirstChild('ForceField')) and (not target.Character:FindFirstChild('ForceField')) and (not target.Character:FindFirstChild('SafeZoneShield')) and LocalPlayer.Character:FindFirstChild('Humanoid') and LocalPlayer.Character.Humanoid.Health > 0 and target.Character:FindFirstChild('Humanoid') and target.Character.Humanoid.Health > 0 then
                        target.Character.HumanoidRootPart.CFrame = CFrame.new(0, 1e5, 0)
                        for _, sphere in pairs(workspace.Storage:GetChildren()) do
                            task.spawn(function()
                                if sphere.Name == getball() then
                                    firetouchinterest(target.Character.HumanoidRootPart, sphere, 0)
                                end
                            end)
                        end
                    end
                end)
            end
        end)
    end
end)

Toggls.FFAbuse:OnChanged(function()
    pcall(function()
        if Flags['ForceFieldAbuse'] then
            RemoteEvent:FireServer({'Respawn'})
        end
    end)
end)

Toggls.Spectate:OnChanged(function()
    pcall(function()    
        if Flags['Spectate'] then 
            if Players[targets[1]].Character and Players[targets[1]].Character:FindFirstChild('Humanoid') and Players[targets[1]].Character.Humanoid.Health > 0 then
                Camera.CameraSubject = Players[targets[1]].Character
            end
            diedt = Players[targets[1]].CharacterAdded:Connect(function(character)
                Camera.CameraSubject = character:WaitForChild('Humanoid', math.huge)
            end)
            diedp = LocalPlayer.CharacterAdded:Connect(function()
                if Players[targets[1]].Character and Players[targets[1]].Character:FindFirstChild('Humanoid') and Players[targets[1]].Character.Humanoid.Health > 0 then
                    Camera.CameraSubject = Players[targets[1]].Character.Humanoid
                end
            end)
            specf = Camera:GetPropertyChangedSignal('CameraSubject'):Connect(function()
                if Players[targets[1]].Character and Players[targets[1]].Character:FindFirstChild('Humanoid') and Players[targets[1]].Character.Humanoid.Health > 0 then
                    Camera.CameraSubject = Players[targets[1]].Character.Humanoid
                end
            end)
        else
            if diedt then
                diedt:Disconnect()
            end
            if diedp then
                diedp:Disconnect()
            end
            if specf then
                specf:Disconnect()
            end
            Camera.CameraSubject = LocalPlayer.Character
        end
    end)
end)

Toggls.SoulAttack:OnChanged(function()
    while Flags['SoulAttack'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets) do
                LocalPlayer.Character.HumanoidRootPart.CFrame = Players[v].Character.HumanoidRootPart.CFrame + Players[v].Character.HumanoidRootPart.CFrame.upVector * 40
                RemoteEvent:FireServer({'Skill_SoulAttack_Start', Players[v]})
                RemoteEvent:FireServer({'Skill_SoulAttack_End'})
            end
        end)
    end
end)

Toggls.SoulReap:OnChanged(function()
    while Flags['SoulReap'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets)do
                if workspace:FindFirstChild(v) then
                    if workspace[v].Humanoid.Health > 0 and not workspace[v]:FindFirstChild('ForceField') and not workspace[v]:FindFirstChild('SafeZoneShield') then
                        RemoteEvent:FireServer({'Skill_SoulReap', Players[v]})
                        repeat task.wait(0) until workspace[v].Humanoid.Health == 0 or not Flags['SoulReap']
                    end
                end
            end
        end)
    end
end)

Toggls.HellFire:OnChanged(function()
    while Flags['HellFire'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets) do
                if v:FindFirstChild('PrivateStats').PsychicPower.Value <= LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value * 100 then
                    RemoteEvent:FireServer({'Skill_DevilFlame', Players[v.Name]})
                end
            end
        end)
    end
end)

Toggls.SpamTP:OnChanged(function()
    while Flags['SpamTP'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets) do
                LocalPlayer.Character.HumanoidRootPart.CFrame = Players[v].Character.HumanoidRootPart.CFrame
            end
        end)
    end
end)

Toggls.SitOnHead:OnChanged(function()
    while Flags['SitOnHead'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(targets) do
                if LocalPlayer.Character:FindFirstChild('Humanoid') then
                    LocalPlayer.Character.Humanoid.Sit = true
                    if workspace[v].Head then
                        LocalPlayer.Character.HumanoidRootPart.CFrame = workspace[v].Head.CFrame * CFrame.new(0, 2.5, 0)
                    end
                end
            end
        end)
    end
end)

Toggls.CBringAll:OnChanged(function()
    while Flags['CBringAll'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(Players:GetPlayers()) do
                if v.Character and not LocalPlayer.Character:FindFirstChild('ForceField') and not v.Character:FindFirstChild('ForceField') and not v.Character:FindFirstChild('SafeZoneShield') and v.Character.Humanoid.Health ~= 0 and v ~= LocalPlayer then
                    v.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame + LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 1
                end
            end
        end)
    end
end)

Toggls.StackAll:OnChanged(function()
    while Flags['StackAll'] and task.wait(0) do
        RemoteEvent:FireServer({'Skill_SpherePunch', Vector3.new(0, 1e20, 0)})
        for _, v in pairs(Players:GetPlayers()) do
            task.spawn(function()
                pcall(function()
                    if v ~= LocalPlayer then
                        if v:FindFirstChild('PrivateStats').BodyToughness.Value <= LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value * 1.5 then
                            if v.Character and LocalPlayer.Character and (not LocalPlayer.Character:FindFirstChild('ForceField')) and (not v.Character:FindFirstChild('ForceField')) and (not v.Character:FindFirstChild('SafeZoneShield')) and LocalPlayer.Character:FindFirstChild('Humanoid') and LocalPlayer.Character.Humanoid.Health > 0 and v.Character:FindFirstChild('Humanoid') and v.Character.Humanoid.Health > 0 then
                                v.Character.HumanoidRootPart.CFrame = CFrame.new(0, 1e9, 0)
                                for _, sphere in pairs(workspace.Storage:GetChildren()) do
                                    task.spawn(function()
                                        if sphere.Name == getball() then
                                            firetouchinterest(v.Character.HumanoidRootPart, sphere, 0)
                                        end
                                    end)
                                end
                            end
                        end
                    end
                end)
            end)
        end
    end
end)

Toggls.SoulAttackAll:OnChanged(function()
    while Flags['SoulAttackAll'] and task.wait(0) do
        pcall(function()
            for _, v in pairs(Players:GetPlayers()) do
                if v ~= LocalPlayer then
                    if v:FindFirstChild('PrivateStats').PsychicPower.Value <= LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value * 10 then
                        if not v.Character:FindFirstChild('SafeZoneShield', true) and not v.Character:FindFirstChild('ForceField', true) then
                            LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame + v.Character.HumanoidRootPart.CFrame.upVector * 40   
                            RemoteEvent:FireServer({'Skill_SoulAttack_Start', Players[v.Name]})
                            RemoteEvent:FireServer({'Skill_SoulAttack_End'})
                        end
                    end
                end
            end
        end)
    end
end)

Toggls.SoulReapAll:OnChanged(function()
    while Flags['SoulReapAll'] and task.wait(0) do
        task.spawn(function()
            pcall(function()
                for _, v in pairs(Players:GetPlayers()) do
                    if v ~= LocalPlayer then
                        if v:FindFirstChild('PrivateStats').PsychicPower.Value <= LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value * 1000 then
                            RemoteEvent:FireServer({'Skill_SoulReap', Players[v.Name]})
                        end
                    end
                end
            end)
        end)
    end
end)

