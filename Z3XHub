local updating_stats = {
    {'FistStrength', 'Fist'},
    {'BodyToughness', 'Body'},
    {'PsychicPower', 'Psychic'}
}

for _, v in pairs(Players:GetPlayers()) do
    task.spawn(function()
        if v ~= LocalPlayer then
            local PrivateStats = v:WaitForChild('PrivateStats', math.huge)
            local leaderstats = v:WaitForChild('leaderstats', math.huge)
            local Status = leaderstats:WaitForChild('Status', math.huge)

            for _, v2 in pairs(updating_stats) do
                PrivateStats:WaitForChild(v2[1], math.huge):GetPropertyChangedSignal('Value'):Connect(function()
                    if game.CoreGui.ESPFolder:FindFirstChild(v.Name) and game.CoreGui.ESPFolder[v.Name]:FindFirstChild('Frame') and game.CoreGui.ESPFolder[v.Name].Frame:FindFirstChild(v2[2]) then
                        game.CoreGui.ESPFolder[v.Name].Frame[v2[2]].Text = v2[2] .. ': ' .. converttoletter(v:FindFirstChild('PrivateStats')[v2[1]].Value)
                    end
                end)
            end

        Status:GetPropertyChangedSignal('Value'):Connect(function()
                if game.CoreGui.ESPFolder:FindFirstChild(v.Name) and game.CoreGui.ESPFolder[v.Name]:FindFirstChild('Frame') and game.CoreGui.ESPFolder[v.Name].Frame:FindFirstChild('Names') then
                    game.CoreGui.ESPFolder[v.Name].Frame.Names.TextColor3 = updateColor(v.leaderstats.Status.Value)
                    game.CoreGui.ESPFolder[v.Name].Frame.Names.TextStrokeColor3 = updateColorStroke(v.leaderstats.Status.Value)
                end
                if v.Character and v.Character.Humanoid and v.Character.Humanoid.Health > 0 then
                    for _, v2 in pairs(v.Character:GetChildren()) do
                        if v2.Name == 'Highlight' then
                            v2.FillColor = updateColor(v.leaderstats.Status.Value)   
                            v2.OutlineColor = updateColor(v.leaderstats.Status.Value)
                        end
                    end
                end
            end)

            if v.Character and v.Character.Humanoid and v.Character.Humanoid.Health > 0 then
                v.Character.Humanoid.Died:Connect(function()
                    game.StarterGui:SetCore('ChatMakeSystemMessage', ({Text = v.DisplayName .. ' (@' .. v.Name .. ') Dead', Color = Color3.fromRGB(255, 0, 0), Font = 'GothamBold', TextSize = 20}))
                end)
            end

            v.CharacterAdded:Connect(function(character)
                if Flags['Esp'] then
                    CreateESP(character, v)
                end
                if Flags['Highlights'] then
                    CreateHighlight(character, v)
                end
                character:WaitForChild('Humanoid', math.huge).Died:Connect(function()
                    game.StarterGui:SetCore('ChatMakeSystemMessage', ({Text = v.DisplayName .. ' (@' .. v.Name .. ') Dead', Color = Color3.fromRGB(255, 0, 0), Font = 'GothamBold', TextSize = 20}))
                end)
            end)
        end
    end)
end

Players.PlayerAdded:Connect(function(player)
    game.StarterGui:SetCore('ChatMakeSystemMessage', {Text = player.DisplayName .. ' (@' .. player.Name .. ') Has joined',Color = Color3.fromRGB(0, 255, 26),Font = 'GothamBold',TextSize = 20})

    if spy then
        player.Chatted:Connect(function(msg) onChatted(player, msg) end)
    end
    
    player.CharacterAdded:Connect(function(character)
        if Flags['Esp'] then
            CreateESP(character, player)
        end
        if Flags['Highlights'] then
            CreateHighlight(character, player)
        end
        character:WaitForChild('Humanoid', math.huge).Died:Connect(function()
            game.StarterGui:SetCore('ChatMakeSystemMessage', ({Text = player.DisplayName .. ' (@' .. player.Name .. ') Dead', Color = Color3.fromRGB(255, 0, 0), Font = 'GothamBold', TextSize = 20}))
        end)
    end)

    local PrivateStats = player:WaitForChild('PrivateStats', math.huge)
    local leaderstats = player:WaitForChild('leaderstats', math.huge)
    local Status = leaderstats:WaitForChild('Status', math.huge)

    for _, v in pairs(updating_stats) do
        PrivateStats:WaitForChild(v[1], math.huge):GetPropertyChangedSignal('Value'):Connect(function()
            if game.CoreGui.ESPFolder:FindFirstChild(player.Name) and game.CoreGui.ESPFolder[player.Name]:FindFirstChild('Frame') and game.CoreGui.ESPFolder[player.Name].Frame:FindFirstChild(v[2]) then
                game.CoreGui.ESPFolder[player.Name].Frame[v[2]].Text = v[2] .. ': ' .. converttoletter(player:FindFirstChild('PrivateStats')[v[1]].Value)
            end
        end)
    end

    Status:GetPropertyChangedSignal('Value'):Connect(function()
        if game.CoreGui.ESPFolder:FindFirstChild(player.Name) and game.CoreGui.ESPFolder[player.Name]:FindFirstChild('Frame') and game.CoreGui.ESPFolder[player.Name].Frame:FindFirstChild('Names') then
            game.CoreGui.ESPFolder[player.Name].Frame.Names.TextColor3 = updateColor(player.leaderstats.Status.Value)
            game.CoreGui.ESPFolder[player.Name].Frame.Names.TextStrokeColor3 = updateColorStroke(player.leaderstats.Status.Value)
        end
        if workspace:FindFirstChild(player.Name) and workspace[player.Name].Humanoid and workspace[player.Name].Humanoid.Health > 0 then
            for _, v2 in pairs(workspace[player.Name]:GetChildren()) do
                if v2.Name == 'Highlight' then
                    v2.FillColor = updateColor(player.leaderstats.Status.Value)   
                    v2.OutlineColor = updateColor(player.leaderstats.Status.Value)
                end
            end
        end
    end)
end)

Players.PlayerRemoving:Connect(function(player)
    game.StarterGui:SetCore('ChatMakeSystemMessage', {Text = player.DisplayName .. ' (@' .. player.Name .. ') Has left',Color = Color3.fromRGB(255, 0, 0),Font = 'GothamBold',TextSize = 20})
    for _, v in pairs(game.CoreGui.ESPFolder:GetChildren()) do
        if v.Name == player.Name then
            v:Destroy()
        end
    end
    if table.find(targets, player.Name) then
        for _, v in pairs(targets) do
            if v == player.Name then
                table.remove(targets, i)
            end
        end
    end
end)

function is_aura_enabled()
    for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
        if v.Name ~= 'KillingIntentAuraSound' and v.Name:match('Aura') then
            return true
        end
    end
end

LocalPlayer.CharacterAdded:Connect(function(character)
    local teleported = false
    if Flags['Stack'] then
        task.spawn(function()
            local savepos = nil
            local body = Instance.new('BodyVelocity')
            body.Name = 'StackVel'
            body.Parent = character:WaitForChild('HumanoidRootPart', math.huge)
            body.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            while Flags['Stack'] and task.wait(0) do
                body.Velocity = Vector3.new(math.random(-20000, 4000), math.random(2000, 4000), math.random(-16000, 4000))
            end
        end)
    end

    if Flags['ForceFieldAbuse'] then
        task.spawn(function()
            --repeat task.wait(0) until LocalPlayer.Character:FindFirstChild('ForceField')
            --repeat task.wait(0) until not LocalPlayer.Character:FindFirstChild('ForceField')
            task.wait(4)
            RemoteEvent:FireServer({'Respawn'})
        end)
    end

    if savepos and not Flags['ForceFieldAbuse'] and not Flags['Stack'] then
        task.spawn(function()
            character:WaitForChild('HumanoidRootPart', math.huge):GetPropertyChangedSignal('CFrame'):Wait()
            character.HumanoidRootPart.CFrame = savepos
            teleported = true
        end)
        task.spawn(function()
            task.wait(1)
            if not teleported and savepos then
                character.HumanoidRootPart.CFrame = savepos
            end
        end)
    end
    
    if Flags['Spin'] then 
        task.spawn(function()
            local bodyang = Instance.new('BodyAngularVelocity')
            bodyang.Name = 'Spin'
            bodyang.Parent = character:WaitForChild('HumanoidRootPart', math.huge)
            bodyang.MaxTorque = Vector3.new(0, math.huge, 0)
            bodyang.AngularVelocity = Vector3.new(0, Flags['SpinSpeed'], 0)
        end)
    end

    if Flags['AutoSuperAura'] then
        task.spawn(function()
            task.wait(0.5)
            RemoteEvent:FireServer({'ConcealRevealAura', 'Start'})
        end)
    elseif Flags['AutoDisableAura'] then
        task.spawn(function()
            repeat task.wait(0) until is_aura_enabled()
            RemoteEvent:FireServer({'ConcealRevealAura', 'Start'})
        end)
    end

    if unpack(Flags['Animations']) ~= 'None' then
        task.spawn(function()
            for _, v in pairs(animations[unpack(Flags['Animations']):lower()]) do
                local action, subAction, assetId = unpack(v)
                LocalPlayer.Character:FindFirstChild('Animate')[action][subAction].AnimationId = assetId
            end
        end)
    end

    if not Flags['ForceFieldAbuse'] and not Flags['Stack'] then
        task.spawn(function()
            func = character:WaitForChild('Humanoid', math.huge):GetPropertyChangedSignal('Health'):Connect(function()
                if character.Humanoid.MaxHealth * respawnhp > character.Humanoid.Health then
                    func:Disconnect()
                    savepos = character.HumanoidRootPart.CFrame
                    character.HumanoidRootPart.CFrame = CFrame.new(-1e9, -1e6, 1e9)
                    RemoteEvent:FireServer({'Respawn'})
                end
            end)
        end)
    end
end)

workspace.Storage.ChildAdded:Connect(function(child)
    if child.Name:match('EnergySphere') then
        task.wait(0)
        if Flags['BlackBalls'] then
            child.Color = Color3.fromHSV(0, 0, 0)
            child.Transparency = 0
        elseif Flags['RGBBalls'] then
            while table.find(workspace.Storage:GetChildren(), child) and task.wait(0) do
                child.Color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
                child.Transparency = 0
            end
        end
    end
end)

if LocalPlayer.Character then
    RemoteEvent:FireServer({'Respawn'})
end

