            LocalPlayer.PlayerGui.ScreenGui.WeeklyQuestFrame.MaxFrame9.RewardTxt.GemImgBtn.ImageColor3 = Color3.fromRGB(0, 0, 0)
            LocalPlayer.PlayerGui.ScreenGui.WeeklyQuestFrame.MaxFrame9.RewardTxt.GemImgBtn.AmountTxt.TextColor3 = Color3.fromRGB(10, 10, 10)
            LocalPlayer.PlayerGui.ScreenGui.WeeklyQuestFrame.MaxFrame9.RewardTxt.GemImgBtn.AmountTxt.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
            Corner.Parent = LocalPlayer.PlayerGui.ScreenGui.MenuFrame.InfoFrame.RankImgBtn
            Corner.CornerRadius = UDim.new(1, 0)
            UIStroke.Parent = LocalPlayer.PlayerGui.ScreenGui.MenuFrame.InfoFrame.RankImgBtn
            UIStroke.Color = Color3.fromRGB(100, 100, 100)
            UIStroke.Thickness = 2.5
            if LocalPlayer.PlayerGui.ScreenGui.MenuBtn then
                LocalPlayer.PlayerGui.ScreenGui.MenuBtn.BorderColor3 = Color3.fromRGB(0, 0, 0)
                LocalPlayer.PlayerGui.ScreenGui.MenuBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
            end
            if LocalPlayer.PlayerGui.ScreenGui.CurrentGemImgBtn.AmountTxtBtn then
                LocalPlayer.PlayerGui.ScreenGui.CurrentGemImgBtn.AmountTxtBtn.TextColor3 = Color3.fromRGB(10, 10, 10)
                LocalPlayer.PlayerGui.ScreenGui.CurrentGemImgBtn.AmountTxtBtn.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
                LocalPlayer.PlayerGui.ScreenGui.CurrentGemImgBtn.ImageColor3 = Color3.fromRGB(0, 0, 0)
            end
        end)
    end,
})

