repeat task.wait(0) until game:IsLoaded()

local Players = game:GetService('Players')
local LocalPlayer = Players.LocalPlayer
local StockGravity = 196.1999969482422
local RunService = game:GetService('RunService')
local UserInputService = game:GetService('UserInputService')
local ReplicatedStorage = game:GetService('ReplicatedStorage')
local RemoteEvent = ReplicatedStorage.RemoteEvent
local HttpService = game:GetService('HttpService')
local VirtualUser = game:GetService('VirtualUser')
local TeleportService = game:GetService('TeleportService')
local Lighting = game:GetService('Lighting')
local Camera = workspace.CurrentCamera

for _, player in pairs(Players:GetPlayers()) do
    task.spawn(function()
        if player ~= LocalPlayer then
            player:WaitForChild('LocalSoundFolder', math.huge):Destroy()
        end
    end)
end

Players.PlayerAdded:Connect(function(player)
    player:WaitForChild('LocalSoundFolder', math.huge):Destroy()
end)

LocalPlayer:WaitForChild('PlayerGui', math.huge):WaitForChild('BubbleChat', math.huge).DescendantAdded:Connect(function(Descendant)
    if Descendant:IsA('ImageLabel') then
        Descendant.ImageColor3 = Color3.fromRGB(0,0,0)
    elseif Descendant:IsA('TextLabel') then
        Descendant.TextColor3 = Color3.fromRGB(255,255,255)
    end
end)

for _, object in pairs(workspace:GetChildren()) do
    if object.Name:match('SoulAttackBeam') and object:IsA('Beam') then
        object:Destroy()
    end
end

workspace.ChildAdded:Connect(function(child)
    task.wait(.1)
    if child.Name:match('SoulAttackBeam') and child:IsA('Beam') then
        child:Destroy()
    end
end)

workspace.FallenPartsDestroyHeight = math.huge - math.huge

LocalPlayer.Idled:Connect(function()
    VirtualUser:Button2Down(Vector2.new(0, 0), Camera.CFrame)
    task.wait(1)
    VirtualUser:Button2Up(Vector2.new(0, 0), Camera.CFrame)
end)

local Rayfield = loadstring(game:HttpGet('https://pastebin.com/raw/M1z7a7WX'))()
Flags = Rayfield['Flags']

if getgenv().Loaded then
    TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId)
end

function round(num, numDecimalPlaces)
    local mult = 10 ^ (numDecimalPlaces or 0)
    local direction = numDecimalPlaces < 0 and math.ceil or math.floor
    return direction(num * mult + 0.5) / mult
end

function converttoletter(num)
    local roundedNum = math.floor(num * 100 + 0.5) / 100
    if roundedNum / 1e21 >= 1 then
        return string.format('%.2fSx', roundedNum / 1e21)
    elseif roundedNum / 1e18 >= 1 then
        return string.format('%.2fQi', roundedNum / 1e18)
    elseif roundedNum / 1e15 >= 1 then
        return string.format('%.2fQa', roundedNum / 1e15)
    elseif roundedNum / 1e12 >= 1 then
        return string.format('%.2fT', roundedNum / 1e12)
    elseif roundedNum / 1e09 >= 1 then
        return string.format('%.2fB', roundedNum / 1e09)
    elseif roundedNum / 1e06 >= 1 then
        return string.format('%.2fM', roundedNum / 1e06)
    elseif roundedNum / 1e03 >= 1 then
        return string.format('%.2fK', roundedNum / 1e03)
    else
        return tostring(math.floor(roundedNum))
    end
end

local statuscolors = {
    ['Criminal'] = Color3.new(1, 0.454902, 0.454902),
    ['Lawbreaker'] = Color3.new(1, 0.662745, 0.388235),
    ['Guardian'] = Color3.new(0.631373, 1, 0.631373),
    ['Protector'] = Color3.new(1, 0.988235, 0.627451),
    ['Supervillain'] = Color3.new(1, 0, 0),
    ['Superhero'] = Color3.new(0, 1, 0.968627), 
}

function updateColor(status) 
    local color = statuscolors[status]
    if color then
        return color
    else
        return Color3.new(1, 1, 1)
    end
end

local statuscolorss = {
    ['Criminal'] = Color3.new(0, 0, 0),
    ['Lawbreaker'] = Color3.new(0, 0, 0),
    ['Guardian'] = Color3.new(0, 0, 0),
    ['Protector'] = Color3.new(0, 0, 0),
    ['Supervillain'] = Color3.fromRGB(255, 183, 46),
    ['Superhero'] = Color3.fromRGB(4, 30, 255),
}

function updateColorStroke(status) 
    local color = statuscolorss[status]
    if color then
        return color
    else
        return Color3.new(0, 0, 0)
    end
end

function formattime(_time)
    local days = math.floor(_time / 86400)
    local hours = math.floor(math.fmod(_time, 86400) / 3600)
    local minutes = math.floor(math.fmod(_time, 3600) / 60)
    local seconds = math.floor(math.fmod(_time, 60))
    return string.format('%02dd : %02dh : %02dm : %02ds',days,hours,minutes,seconds)
end

local espfolder = Instance.new('Folder', game.CoreGui)
espfolder.Name = 'ESPFolder'
espfolder.ChildAdded:Connect(function(child)
    if not Flags['Esp'] then
        child:Destroy()
    end
end)

function CreateESP(character, player)
    task.spawn(function()
        pcall(function()
            local BillboardGui = Instance.new('BillboardGui', game.CoreGui.ESPFolder)
            BillboardGui.Adornee = character:WaitForChild('Head', math.huge)
            BillboardGui.ExtentsOffset = Vector3.new(0, 1, 0)
            BillboardGui.AlwaysOnTop = true
            BillboardGui.Size = UDim2.new(0, 2, 0, 2)
            BillboardGui.StudsOffset = Vector3.new(0, 3, 0)
            BillboardGui.Name = character.Name
            
            local frame = Instance.new('Frame', BillboardGui)
            frame.ZIndex = 10
            frame.BackgroundTransparency = 1
            frame.Size = UDim2.new(5, 0, 5, 0)
            
            local TxtName = Instance.new('TextLabel', frame)
            TxtName.Name = 'Names'
            TxtName.ZIndex = 10
            TxtName.Text = character.Name
            TxtName.BackgroundTransparency = 1
            TxtName.Position = UDim2.new(0, 0, 0, -55)
            TxtName.Size = UDim2.new(1, 0, 10, 0)
            TxtName.Font = 'GothamBold'
            TxtName.TextSize = 14
            TxtName.TextStrokeTransparency = 0.5
            TxtName.TextColor3 = updateColor(player.leaderstats.Status.Value)
            TxtName.TextStrokeColor3 = updateColorStroke(player.leaderstats.Status.Value)
            
            local TxtDist = Instance.new('TextLabel', frame)
            TxtDist.Name = 'Dist'
            TxtDist.ZIndex = 10
            TxtDist.Text = ''
            TxtDist.BackgroundTransparency = 1
            TxtDist.Position = UDim2.new(0, 0, 0, -45)
            TxtDist.Size = UDim2.new(1, 0, 10, 0)
            TxtDist.Font = 'GothamBold'
            TxtDist.TextColor3 = Color3.new(1, 1, 1)
            TxtDist.TextSize = 14
            TxtDist.TextStrokeTransparency = 0.5
            
            local TxtHealth = Instance.new('TextLabel', frame)
            TxtHealth.Name = 'Health'
            TxtHealth.ZIndex = 10
            TxtHealth.Text = 'Health: ' .. converttoletter(character:WaitForChild('Humanoid', math.huge).Health) .. '/' .. converttoletter(character:WaitForChild('Humanoid', math.huge).MaxHealth)
            TxtHealth.BackgroundTransparency = 1
            TxtHealth.Position = UDim2.new(0, 0, 0, -35)
            TxtHealth.Size = UDim2.new(1, 0, 10, 0)
            TxtHealth.Font = 'GothamBold'
            TxtHealth.TextColor3 = Color3.new(1, 1, 1)
            TxtHealth.TextSize = 14
            TxtHealth.TextStrokeTransparency = 0.5
            
            local TxtBody = Instance.new('TextLabel', frame)
            TxtBody.Name = 'Body'
            TxtBody.ZIndex = 10
            TxtBody.BackgroundTransparency = 1
            TxtBody.Position = UDim2.new(0, 0, 0, -25)
            TxtBody.Size = UDim2.new(1, 0, 10, 0)
            TxtBody.Font = 'GothamBold'
            TxtBody.TextColor3 = Color3.new(1, 1, 1)
            TxtBody.TextSize = 14
            TxtBody.TextStrokeTransparency = 0.5
            TxtBody.Text = 'Body: ' .. converttoletter(player:FindFirstChild('PrivateStats').BodyToughness.Value)
            
            local TxtFist = Instance.new('TextLabel', frame)
            TxtFist.Name = 'Fist'
            TxtFist.ZIndex = 10
            TxtFist.BackgroundTransparency = 1
            TxtFist.Position = UDim2.new(0, 0, 0, -15)
            TxtFist.Size = UDim2.new(1, 0, 10, 0)
            TxtFist.Font = 'GothamBold'
            TxtFist.TextColor3 = Color3.new(1, 1, 1)
            TxtFist.TextSize = 14
            TxtFist.TextStrokeTransparency = 0.5
            TxtFist.Text = 'Fist: ' .. converttoletter(player:FindFirstChild('PrivateStats').FistStrength.Value)

            local TxtPsychic = Instance.new('TextLabel', frame)
            TxtPsychic.Name = 'Psychic'
            TxtPsychic.ZIndex = 10
            TxtPsychic.BackgroundTransparency = 1
            TxtPsychic.Position = UDim2.new(0, 0, 0, -5)
            TxtPsychic.Size = UDim2.new(1, 0, 10, 0)
            TxtPsychic.Font = 'GothamBold'
            TxtPsychic.TextColor3 = Color3.new(1, 1, 1)
            TxtPsychic.TextSize = 14
            TxtPsychic.TextStrokeTransparency = 0.5
            TxtPsychic.Text = 'Psychic: ' .. converttoletter(player:FindFirstChild('PrivateStats').PsychicPower.Value)

            local humanoid = character:WaitForChild('Humanoid', math.huge)

            TxtHealth.Text = 'Health: ' .. converttoletter(humanoid.Health) .. '/' .. converttoletter(humanoid.MaxHealth)

            humanoid:GetPropertyChangedSignal('Health'):Connect(function()
                TxtHealth.Text = 'Health: ' .. converttoletter(character.Humanoid.Health) .. '/' .. converttoletter(character.Humanoid.MaxHealth)
                if character.Humanoid.Health <= 0 then
                    BillboardGui:Destroy()
                end
            end)

            while task.wait(0) and table.find(workspace:GetChildren(), character) do
                pcall(function()
                    TxtDist.Text = 'Distance: ' .. math.floor((LocalPlayer.Character.HumanoidRootPart.Position - character.HumanoidRootPart.Position).Magnitude)     
                end)
            end
        end)
    end)
end

function CreateHighlight(character, player)
    task.spawn(function()
        pcall(function()
            local Highlight = Instance.new('Highlight')
            Highlight.Name = 'Highlight'
            Highlight.FillTransparency = 0.5
            Highlight.FillColor = updateColor(player:FindFirstChild('leaderstats').Status.Value)
            Highlight.OutlineColor = updateColor(player:FindFirstChild('leaderstats').Status.Value)
            Highlight.Parent = character

            character:WaitForChild('Humanoid', math.huge):GetPropertyChangedSignal('Health'):Connect(function()
                character.Humanoid.Died:Connect(function()
                    local highlight = character:FindFirstChildOfClass('Highlight')

                    if highlight then
                        highlight:Destroy()
                    end
                end)
            end)
        end)
    end)
end

task.spawn(function()
    for i,v in pairs(game.CoreGui:GetChildren()) do 
        if v.Name == 'Stats' then
            v:Destroy()
        end
    end

    local ScreenGui = Instance.new('ScreenGui')
    local TextLabel = Instance.new('TextLabel')
    local UICorner = Instance.new('UICorner')
    
    ScreenGui.Parent = game.CoreGui
    ScreenGui.Name = 'Stats'
    
    TextLabel.Parent = ScreenGui
    TextLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.BackgroundTransparency = 0
    TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextLabel.BorderSizePixel = 0   
    TextLabel.Position = UDim2.new(0.37, 0, -0.05, 0)
    TextLabel.Size = UDim2.new(0.27, 0, 0.03, 0)
    TextLabel.Font = 'GothamBold'
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextScaled = false
    TextLabel.TextSize = 15
    TextLabel.TextWrapped = true

    UICorner.Parent = TextLabel

    local frameCount = 0
    local startTime = tick()
    
    RunService.RenderStepped:Connect(function()
        frameCount = frameCount + 1
        if tick() - startTime >= 1 then
            FpsCap = 'Fps: ' .. frameCount
            Ping = 'Ping: ' .. math.floor(tonumber(game.Stats:FindFirstChild('PerformanceStats').Ping:GetValue()))
            TextLabel.Text = 'Z3XHub ┃ Release ┃ ' .. FpsCap .. ' ┃ ' .. Ping .. ' ┃ ' .. os.date('%H:%M:%S', os.time())
            frameCount = 0
            startTime = tick()
        end
    end)  
end)

function getfs()
    if LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e13 then
        return {'+FS6'}
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e11 then
        return {'+FS5'}
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e9 then
        return {'+FS4'}
    else
        return {'+FS3', '+FS2', '+FS1'}
    end
end

function getpp()
    if LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e15 then
        return {'+PP6'}
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e12 then
        return {'+PP5'}
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e9 then
        return {'+PP4'}
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e6 then
        return {'+PP3'}
    else
        return {'+PP2', '+PP1'}
    end
end

function getfspos()
    if LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e13 then
        return -364.097198, 15735.1455, -7.32395744
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e11 then
        return 1380.3583984375, 9273.736328125, 1652.6295166015625
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value >= 1e9 then
        return 1175.2962646484375, 4789.25341796875, -2293.052978515625
    end
end

function getbtpos()
    if LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e13 then
        return -278.3638916015625, 281.3877868652344, 1005.1320190429688
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e11 then
        return -275.5314025878906, 281.6444396972656, 991.4784545898438
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e9 then
        return -244.4465789794922, 287.1148986816406, 978.9732666015625
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e7 then
        return -2024.790283203125, 714.248779296875, -1882.2078857421875
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e6 then
        return -2297.46484375, 976.7352294921875, 1075.0213623046875
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e5 then
        return 1634.4921875, 259.62298583984375, 2248.2060546875
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e4 then
        return 357.2840881347656, 263.7413635253906, -492.33245849609375
    elseif LocalPlayer:FindFirstChild('PrivateStats').BodyToughness.Value >= 1e2 then
        return 367.770751953125, 249.7052459716797, -444.747802734375
    end
end

function getpppos()
    if LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e15 then
        return -2545.082763671875, 5412.32958984375, -491.8777160644531
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e12 then
        return -2581.781005859375, 5516.38818359375, -500.4078674316406
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e9 then
        return -2561.86328125, 5500.8828125, -440.46112060546875
    elseif LocalPlayer:FindFirstChild('PrivateStats').PsychicPower.Value >= 1e6 then
        return -2529.778076171875, 5486.39208984375, -534.6697998046875
    end
end

function getball()
    if LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e15 then
        return 'EnergySphere9'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e12 then
        return 'EnergySphere8'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e10 then
        return 'EnergySphere7'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e8 then
        return 'EnergySphere6'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e7 then
        return 'EnergySphere5'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e6 then
        return 'EnergySphere4'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e5 then
        return 'EnergySphere3'
    elseif LocalPlayer:FindFirstChild('PrivateStats').FistStrength.Value > 1e4 then
        return 'EnergySphere2'
    else
        return 'EnergySphere1'
    end
end

