Farm:CreateButton({
    Name = 'GodMode',
    Callback = function()
        task.spawn(function()
            Players.LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.Text = 'Press to activate GodMode'
            Players.LocalPlayer.PlayerGui.ScreenGui.QuestTalkBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
            ReplicatedStorage.GhostRiderPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame
            Players.LocalPlayer.PlayerGui.ScreenGui.QuestMsgFrame:GetPropertyChangedSignal('Visible'):Connect(function()
                ReplicatedStorage.GhostRiderPart.CFrame = CFrame.new( 147.031021, 249.383453, 1235.40491, 0.00243985653, -0, -0.999997139, 0, 1, -0, 0.999997139, 0, 0.00243985653)
                task.wait(1)
                Camera.CameraSubject = LocalPlayer.Character.Humanoid
                Camera.CameraType = Enum.CameraType.Custom
                task.wait(.1)
                LocalPlayer.PlayerGui.ScreenGui.QuestMsgFrame.Visible = false
                return
            end)
        end)
    end,
})


Visual:CreateButton({
    Name = 'Better Sky',
    Callback = function()
        local Skybox = Instance.new('Sky', Lighting)
        local skyboxTextures = {
            SkyboxBk = 'http://www.roblox.com/asset/?id=159454299',
            SkyboxDn = 'http://www.roblox.com/asset/?id=159454296',
            SkyboxFt = 'http://www.roblox.com/asset/?id=159454293',
            SkyboxLf = 'http://www.roblox.com/asset/?id=159454286',
            SkyboxRt = 'http://www.roblox.com/asset/?id=159454300',
            SkyboxUp = 'http://www.roblox.com/asset/?id=159454288'
        }
        for direction, textureUrl in pairs(skyboxTextures) do
            Skybox[direction] = textureUrl
        end
        Skybox.CelestialBodiesShown = false
        if Lighting:FindFirstChild('SunRays') then
            Lighting.SunRays:Destroy()
        end
    end,
})

Visual:CreateButton({
    Name = 'Black Map',
    Callback = function()
        pcall(function()
            workspace.Terrain.WaterColor = Color3.new(0, 0, 0)
            workspace.Terrain.WaterTransparency = 0            
            workspace.Main.TrainingCrystal.Part.Color = Color3.new(0, 0, 0)
            workspace.Main.TrainingRock.Color = Color3.new(0, 0, 0)

            for _, v in pairs({workspace.Town.Halloween, workspace.Main.GhostRiderCollisionPart, workspace.Main.GrimCollisionPart, workspace.Main.Volcano.DangerSign, workspace.Main.MouseIgnoreGroup.NPCBlockPart1, workspace.Main.MouseIgnoreGroup.NPCBlockPart2, workspace.Main.QuestNPC.Sathopian:FindFirstChild('CuteFoxyBeanie')}) do
                v:Destroy()
            end

            for _, v in pairs({workspace.Main.TrainingCrystal.cloud, workspace.Main.TrainingCrystal.CyanGem}) do
                for _, v2 in pairs(v:GetChildren()) do
                    if v2:IsA('BasePart') then
                        v2.BrickColor = BrickColor.new(0, 0, 0)
                    end
                end
            end

            for _, v in pairs({workspace.Main.TrainingArea.StarFSTraining1, workspace.Main.TrainingArea.StarFSTraining2, workspace.Main.TrainingArea.StarFSTraining3}) do
                for _, v2 in pairs({v.Atmosphere1, v.Atmosphere2}) do
                    v2.BrickColor = BrickColor.new(0, 0, 0)
                end
                for _, v2 in pairs({v.WarnBBGui.TextLabel}) do
                    v2.Text = 'Z3XHub'
                end
            end
            
            for _, v in pairs(workspace.Main.Waterfall:GetChildren()) do 
                if v:IsA('BasePart') then
                    v.BrickColor = BrickColor.new(0,0,0)
                    if v.Name == 'Union' then
                        v.UsePartColor = true
                    end
                end
            end

            for _, v in pairs({workspace.Town.Color.Tree1, workspace.Town.Color.Tree2, workspace.Town.Color.Tree3}) do
                for _, v2 in pairs(v.Leaf:GetChildren()) do
                    v2.BrickColor = BrickColor.new(0, 0, 0)
                end
                for _, v2 in  pairs(v.Trunk:GetChildren()) do
                    v2.BrickColor = BrickColor.new(0, 0, 0)
                end
            end

            for _, v in pairs({workspace.Main.Volcano}) do
                for _, v2 in pairs(v:GetChildren()) do
                    if v2.Name == 'LavaFloor' then
                        v2.BrickColor = BrickColor.new(0, 0, 0)
                        v2.UsePartColor = true
                    end
                end
            end

            for _, v in pairs({workspace.Main.Volcano.Parts, workspace.Town.Color.Floor, workspace.Main.TrainingCrystal, workspace.Main.OceanFloor}) do
                for _, v2 in pairs(v:GetChildren()) do
                    if v2:IsA('BasePart') then  
                        v2.BrickColor = BrickColor.new(0, 0, 0)
                    end
                end
            end

            for _, v in pairs({workspace.Main.IceMountain, workspace.Main.Tornado}) do
                for _, v2 in pairs(v:GetChildren()) do
                    if v2:IsA('BasePart') then
                        v2.BrickColor = BrickColor.new(0, 0, 0)
                    end
                end
            end

            for _, v in pairs(workspace.Town.Core:GetDescendants()) do
                if v:IsA('SpotLight') then
                    v.Color = Color3.fromRGB(98, 0, 255)
                    v:GetPropertyChangedSignal('Color'):Connect(function()
                        v.Color = Color3.fromRGB(98, 0, 255)
                    end)
                end
            end
        end)
    end,
})

